{
  "name": "neo4j-node-js-typescript",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start:dev": "nodemon --exec ts-node ./src/server.ts",
    "start:dev_old": "nodemon --watch src/**/*.ts --exec ts-node ./src/server.ts",
    "start:prod": "node ./dist/server.js"
  },
  "dependencies": {
    "express": "4.17.1",
    "neo4j-driver": "^5.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "nodemon": "2.0.15",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "packageManager": "yarn@3.5.0"
}
